<p id="notice"><%= notice %></p>

<h1>Listing Doctor Adds</h1>

<table>
  <thead>
    <tr>
      <th>From</th>
      <th>To</th>
      <th>Total</th>
      <th>Used</th>
      <th>Docname</th>
      <th>Docfamily</th>
      <th>Speciality</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctor_adds.each do |doctor_add| %>
      <tr>
        <td><%= doctor_add.from %></td>
        <td><%= doctor_add.to %></td>
        <td><%= doctor_add.total %></td>
        <td><%= doctor_add.used %></td>
        <td><%= doctor_add.docName %></td>
        <td><%= doctor_add.docFamily %></td>
        <td><%= doctor_add.speciality %></td>
        <td><%= link_to 'Show', doctor_add %></td>
        <td><%= link_to 'Edit', edit_doctor_add_path(doctor_add) %></td>
        <td><%= link_to 'Destroy', doctor_add, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

