      <th>Date</th>
      <th>Doctor</th>
      <th>Reserve</th>
      <th>User</th>
      <th>Turn</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_reserves.each do |user_reserf| %>
      <tr>
        <td><%= user_reserf.date %></td>
