        <td><%= user_reserf.reserve_id %></td>
        <td><%= user_reserf.user_id %></td>
        <td><%= user_reserf.turn %></t
        <td><%= link_to 'Show', user_reserf %></td>
        <td><%= link_to 'Edit', edit_user_reserf_path(user_reserf) %></td>
        <td><%= link_to 'Destroy', user_reserf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

